                <tr>
                    <th>Nombre Del Cliente</th>
                    <th>Cedula</th>
                    <th>Codigo Cuarto</th>
                    <th>Descripcion De Cuarto</th>
                    <th>Descripcion De Daño</th>
                    <th>Fecha de Daño</th>
                    <th>Costo</th>
                </tr>
            </thead>
            <tbody>
            </tbody>
        </table>
    </div>
</form>



